<!-- Template maven's user config file ~/.m2/settings.xml
-->

<settings>
  <servers>
    <server>
      <id>sipana</id>
      <username>mhack</username>
      <privateKey>/home/mhack/.ssh/id_rsa</privateKey>
      <filePermissions>664</filePermissions>
      <directoryPermissions>775</directoryPermissions>
    </server>
  </servers>

  <profiles>
    <profile>
      <id>mhack</id>

      <repositories>
        <repository>
          <id>sipana</id>
          <name>Profile for Sipana project</name>
          <url>http://maven.sipana.org/</url>
          <layout>default</layout>
	    </repository>
      </repositories>
    </profile>
  </profiles>

  <activeProfiles>
    <activeProfile>mhack</activeProfile>
  </activeProfiles>

</settings>

